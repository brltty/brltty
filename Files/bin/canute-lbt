#!/bin/bash
. /brltty/current/libexec/canute-prologue.sh

showProgramUsageNotes() {
cat <<END_OF_PROGRAM_USAGE_NOTES

Show or set the current literary (contraction) braille table.
END_OF_PROGRAM_USAGE_NOTES
}

optionalProgramParameters
addProgramParameter "new-table" newTable "the new literary (contraction) braille table to use"
parseProgramArguments "${@}"

exec canute-parameter literary-braille-table "${newTable}"
exit "${?}"
